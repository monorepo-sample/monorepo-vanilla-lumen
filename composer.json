{
    "authors": [
        {
            "name": "aminalalialdarwish",
            "email": "email@example.com"
        }
    ],
    "require": {
        "bramus/router": "^1.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/lumen-framework": "^8.3.1",
        "pear/http_request2": "^2.5",
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5.10",
        "symplify/monorepo-builder": "11.1.30.72"
    },
    "autoload": {
        "psr-4": {
            "App\\": "packages/backend-lumen/app/",
            "Database\\Factories\\": "packages/backend-lumen/database/factories/",
            "Database\\Seeders\\": "packages/backend-lumen/database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "packages/backend-lumen/tests/"
        ]
    },
    "replace": {
        "bns/frontend-vanilla": "self.version",
        "laravel/lumen": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
